<MudDialog>
    <DialogContent>
        Are you sure you want to delete this budget?
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Dark" Variant="Variant.Filled" StartIcon="@Icons.Material.Filled.Cancel" OnClick="Cancel">
            Cancel
        </MudButton>
        <MudButton Color="Color.Error" Variant="Variant.Filled" StartIcon="@Icons.Material.Filled.Delete" OnClick="() => Delete()">
            Delete
        </MudButton>
    </DialogActions>
</MudDialog>
